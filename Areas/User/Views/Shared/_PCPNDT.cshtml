    function ShowTestsFacilities(facilityId){
        switch (facilityId) {
            case '1':   //Generic Counselling Center
            case '6':   //Assisted Reproductive Technology (ART)
                {
                    if(facilityId){
                        $('#fsTestsRemarks').show();
                        $('#fsFacilityRemarks').show();
                    }
                    else{
                        $('#fsTestsRemarks').hide();
                        $('#fsFacilityRemarks').hide();
                    }
                }
                break;
            case '2':   //Ultra Sound Clinic
            case '3':   //Imaging Center
                {
                    if(facilityId){
                        $('#fsNonInvasive').show();
                        $('#fsFacilityRemarks').show();
                    }
                    else{
                        $('#fsNonInvasive').hide();
                        $('#fsFacilityRemarks').hide();
                    }
                }
                break;
            case '4':   //Genetic Clinic
            case '5':   //Generic Laboratory
                {
                    if(facilityId){
                        $('#fsInvasive').show();
                        $('#fsNonInvasive').show();
                        $('#fsTests').show();
                        $('#fsStudies').show();
                    }
                    else{
                        $('#fsInvasive').hide();
                        $('#fsNonInvasive').hide();
                        $('#fsTests').hide();
                        $('#fsStudies').hide();
                    }
                }
                break;
            default:

        }
    }
